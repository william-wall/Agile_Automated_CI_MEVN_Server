    "chai-things": "^0.2.0",
    "cross-env": "^5.2.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.4.2",
    "nodemon": "^1.18.7",
    "npm-run-all": "^4.1.5",
    "rimraf": "^2.6.2",
    "supertest": "^3.3.0",
    "typescript": "^2.8.1"
  }
