    "boom": "^7.2.0",
    "cors": "^2.8.4",
    "del": "^2.2.2",
    "ejs": "^2.5.7",
    "express": "^4.16.3",
    "gridfs-stream": "^1.1.1",
    "hapi": "^17.3.0",
    "http-errors": "^1.7.1",
    "lokijs": "^1.4.2",
    "method-override": "^3.0.0",
    "mongoose": "^5.2.5",
    "morgan": "^1.8.2",
    "multer": "^1.3.1",
    "multer-gridfs-storage": "^3.2.2",
    "serve-favicon": "^2.5.0",
    "sightengine": "^1.3.1",
    "socket.io": "^2.1.1",
    "uuid": "^3.0.1",
    "lodash": "latest",
    "should": "latest"
  },
  "devDependencies": {
    "@types/boom": "^7.2.0",
    "@types/del": "^3.0.1",
    "@types/hapi": "^17.0.7",
